page{
	background-color: #ccc;
}
.slide{
	height: 350rpx;
	overflow: hidden;
}
.slide image{
	width: 100%;
	height: 350rpx;
}
.index-nav{
	display: flex;
	justify-content: center;
	flex-wrap: wrap;
	background-color: #fff;
}
.nav-item{
	width: 33.33%;
	height: 200rpx;
	border-bottom: 1rpx solid #f2f2f2;
	/* box-sizing: border-box; */
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	font-size: 30rpx;
	color: #666;
	position: relative;
}
.nav-item::after{
	content: "";
	width: 1rpx;
	height:100%;
	background-color:#f2f2f2;
	position: absolute;
	right: 0;
	top:0;
}
.nav-item:nth-child(3n)::after{
	width:0;
}
.index-nav image{
	width:80rpx;
	height: 80rpx;
}

/*模块3首页入口分区*/
.index-enter{
	margin-top: 20rpx;
	padding: 18rpx 10rpx;
	display: flex;
	justify-content: space-around;
	background-color: #fff;
}
.index-enter image{
	width: 320rpx;
	height: 178rpx;
}

/*轮播图*/
.slide swiper{
	width:100%;
	height: 350rpx;
}